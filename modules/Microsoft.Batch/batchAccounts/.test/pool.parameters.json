                    },
                    "startTask": {
                        "commandLine": "cmd /c ipconfig",
                        "maxTaskRetryCount": 4,
                        "waitForSuccess": false
                    },
                    "taskSchedulingPolicy": "Pack",
                    "taskSlotsPerNode": 1,
                    "userAccounts": [
                        {
                            "elevationLevel": "Admin",
                            "name": "carmladmin",
                            "password": "VeryGeheim!",
                            "windowsUserConfiguration": {
                                "loginMode": "Interactive"
